{
  "settings": {
    "name": "Zooz Moving Tucson",
    "currentPage": "movers-in-tucson.html",
    "ai": {
      "prompt": "A website for a home service business. Has a header and different sections. Please ensure to also have footer to display NAP, as well as service pages\n",
      "imageSearchQuery": "service"
    },
    "theme": {
      "name": "startm5",
      "title": "StartM5",
      "styling": {
        "backgroundColor": "#FFF5E8",
        "primaryColor": "#29BF12",
        "secondaryColor": "#F21B3F",
        "successColor": "#20AC6B",
        "infoColor": "#0AA3C2",
        "warningColor": "#CC9900",
        "dangerColor": "#AE1E2C",
        "mainFont": "Roboto Flex",
        "display1Font": "Roboto Flex",
        "display1Size": 5,
        "display2Font": "Roboto Flex",
        "display2Size": 4,
        "display5Font": "Roboto Flex",
        "display5Size": 2.5,
        "display7Font": "Roboto Flex",
        "display7Size": 1.4,
        "display4Font": "Roboto Flex",
